/*. Write a Java program to create a class called "Employee" with a name, job title, and salary
 attributes, and methods to calculate and update salary*/


package ranjith.assignment.three;

public class Question6 {
    private static final String Totalsalary1 = null;
	String Name;
    String jobtitle;
    float Salary; 
    float totalsalary;
     float calclatesalary(float Salary) {
		return Salary*12;
    }
    float Totalsalary1(float total) {
   	totalsalary=calclatesalary(total);
	 return total;
   }
    
	
	
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Question6 sal=new Question6();
		sal.jobtitle="SE";
		sal.Name="ranjith";
		sal.Salary= 12000.34f;
       float total=sal.calclatesalary(sal.Salary);
       System.out.println(total);
        sal.totalsalary=sal.Totalsalary1(total);
        System.out.println(sal.totalsalary);
	}
}




	
